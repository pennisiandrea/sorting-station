  <body>
    <tu tuid="WaitingInit">
      <tuv xml:lang="it">
        <seg>Premere il tasto inizializzazione per abilitare la macchina al lavoro</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Press initialize button to enable machine at work</seg>
      </tuv>
      <tuv xml:lang="de">
        <seg>Servus Welt!</seg>
      </tuv>
    </tu>
    <tu tuid="Init">
      <tuv xml:lang="it">
        <seg>Inizializzazione</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Initializing</seg>
      </tuv>
    </tu>
    <tu tuid="WaitingCap">
      <tuv xml:lang="it">
        <seg>Attesa tappo</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Waiting cap</seg>
      </tuv>
    </tu>
    <tu tuid="Punching">
      <tuv xml:lang="it">
        <seg>In punzonatura</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Punching the cap</seg>
      </tuv>
    </tu>
    <tu tuid="Delivering">
      <tuv xml:lang="it">
        <seg>Consegno il tappo</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Delivering the cap</seg>
      </tuv>
    </tu>
    <tu tuid="WaitingModules">
      <tuv xml:lang="it">
        <seg>Attesa moduli</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Waiting modules ready</seg>
      </tuv>
    </tu>
    <tu tuid="Manual">
      <tuv xml:lang="it">
        <seg>Modalita manuale attiva</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Manual mode active</seg>
      </tuv>
    </tu>
    <tu tuid="WaitingStart">
      <tuv xml:lang="it">
        <seg>Premere il tasto start per iniziare a lavorare</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Press start button to start work</seg>
      </tuv>
    </tu>
    <tu tuid="WaitingMode">
      <tuv xml:lang="it">
        <seg>Selezionare la modalità di lavoro premendo i tasti Modalita automatica o Modalita manuale</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Select the working mode by pressing Automatic mode or Manual mode buttons.</seg>
      </tuv>
    </tu>
    <tu tuid="Error">
      <tuv xml:lang="it">
        <seg>Macchina in errore, premere reset o inizializza.</seg>
      </tuv>
      <tuv xml:lang="en">
        <seg>Machine in error, please press reset or initialize buttons.</seg>
      </tuv>
    </tu>
