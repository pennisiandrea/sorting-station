<?xml version="1.0" encoding="utf-8"?>
<EventBindingSet id="login_content_eventbinding" xmlns="http://www.br-automation.com/iat2014/eventbinding/v2" xmlns:types="http://www.br-automation.com/iat2015/widgetTypes/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Bindings>
    <EventBinding id="login_content.ToggleButton2.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="login_content" widgetRefId="ToggleButton2" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="clientSystem.Action">
            <Method xsi:type="clientSystem.Action.CloseDialog" dialogId="dialog_login" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
    <EventBinding id="login_content.Login1.LoginSuccess">
      <Source xsi:type="widgets.brease.Login.Event" contentRefId="login_content" widgetRefId="Login1" event="LoginSuccess" />
      <EventHandler>
        <Action>
          <Target xsi:type="clientSystem.Action">
            <Method xsi:type="clientSystem.Action.CloseDialog" dialogId="dialog_login" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
  </Bindings>
</EventBindingSet>